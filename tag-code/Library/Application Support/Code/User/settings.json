{
  "editor.fontSize": 20,
  "editor.lineHeight": 26,
  "editor.tabSize": 2,
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 3,
  "editor.wordWrap": "on",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.fontWeight": "400",
  "editor.fontLigatures": true,
  "editor.minimap.enabled": false,
  "editor.cursorBlinking": "solid",
  "editor.colorDecorators": false,
  "editor.snippetSuggestions": "top",
  "editor.renderWhitespace": "none",
  "editor.parameterHints.enabled": false,
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "workbench.settings.editor": "json",
  "workbench.tips.enabled": false,
  "workbench.startupEditor": "none",
  "workbench.layoutControl.enabled": false,
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.revealIfOpen": true,
  "workbench.sideBar.location": "right",
  "workbench.editor.showTabs": "multiple",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "explorer.confirmDelete": false,
  "html.format.enable": true,
  "html.format.preserveNewLines": true,
  "files.trimTrailingWhitespace": true,
  "terminal.integrated.fontSize": 16,
  "eslint.alwaysShowStatus": true,
  "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
  },
  "extensions.ignoreRecommendations": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.next": true,
  },
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-s>"],
      "commands": ["workbench.action.files.save"],
    },
    {
      "before": ["leader", "<space>"],
      "commands": ["workbench.action.quickOpen"],
    },
    {
      "before": ["leader", "f", "r"],
      "commands": ["workbench.action.openRecent"],
    },
    {
      "before": ["leader", "f", "n"],
      "commands": ["workbench.action.files.newUntitledFile"],
    },
    {
      "before": ["leader", "e"],
      "commands": ["workbench.action.toggleSidebarVisibility"],
    },
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"],
    },
  ],
  "vim.normalModeKeyBindings": [
    // {
    //   "before": ["<leader>", "<space>"],
    //   "commands": ["workbench.action.showCommands"],
    //   "silent": true,
    // },
    {
      "before": ["<C-c>"],
      "commands": ["workbench.action.closePan"],
      "when": "inCommandPalette",
      "silent": true,
    },
  ],
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "gitlens.currentLine.enabled": false,
  "explorer.openEditors.visible": 0,
  "explorer.autoReveal": false,
  "vim.disableExtension": false,
  "git.enableSmartCommit": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "go.useLanguageServer": true,
  "go.formatTool": "goimports",
  "vsicons.dontShowNewVersionMessage": true,
  "pgFormatter.keywordCase": "uppercase",
  "pgFormatter.commaBreak": true,
  "settingsSync.ignoredExtensions": [],
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "editor.suggestSelection": "first",
  "editor.fontFamily": "'Dank Mono', 'Fira Code', Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontFamily": "'Dank Mono', 'Fira Code', Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontLigatures.enabled": true,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1",
  },
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.editor.tabActionCloseVisibility": false,
  "window.confirmSaveUntitledWorkspace": false,
  "python.analysis.typeCheckingMode": "standard",
  "workbench.colorTheme": "Catppuccin Mocha",
  "diffEditor.codeLens": true,
  "aws.cloudformation.telemetry.enabled": true,
  "claudeCode.preferredLocation": "panel",
  "totalTypeScript.hideAllTips": true,
  "totalTypeScript.hideBasicTips": true,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1,
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": ["#8FBCBB", "#434C5E"],
  "vim.statusBarColors.insert": "#BF616A",
  "vim.statusBarColors.visual": "#B48EAD",
  "vim.statusBarColors.visualline": "#B48EAD",
  "vim.statusBarColors.visualblock": "#A3BE8C",
  "vim.statusBarColors.replace": "#D08770",
  "vim.statusBarColors.commandlineinprogress": "#007ACC",
  "vim.statusBarColors.searchinprogressmode": "#007ACC",
  "vim.statusBarColors.easymotionmode": "#007ACC",
  "vim.statusBarColors.easymotioninputmode": "#007ACC",
  "vim.statusBarColors.surroundinputmode": "#007ACC",
  "workbench.colorCustomizations": {
    "statusBar.background": "#8FBCBB",
    "statusBar.noFolderBackground": "#8FBCBB",
    "statusBar.debuggingBackground": "#8FBCBB",
    "statusBar.foreground": "#434C5E",
    "statusBar.debuggingForeground": "#434C5E"
  },
}
