{
  "editor.fontSize": 15.5,
  "editor.lineHeight": 24,
  "editor.tabSize": 2,
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 3,
  "editor.wordWrap": "on",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.fontWeight": "400",
  "editor.fontLigatures": true,
  "editor.minimap.enabled": false,
  "editor.cursorBlinking": "solid",
  "editor.colorDecorators": false,
  "editor.snippetSuggestions": "top",
  "editor.renderWhitespace": "none",
  "editor.parameterHints.enabled": false,
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "[typescript]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.settings.editor": "json",
  "workbench.tips.enabled": false,
  "workbench.startupEditor": "none",
  "workbench.editor.tabSizing": "shrink",
  "workbench.sideBar.location": "left",
  "workbench.editor.tabCloseButton": "off",
  "workbench.editor.showTabs": "multiple",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "explorer.confirmDelete": false,
  "html.format.enable": true,
  "html.format.preserveNewLines": true,
  "files.trimTrailingWhitespace": true,
  "terminal.integrated.fontSize": 16,
  "eslint.alwaysShowStatus": true,
  "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "extensions.ignoreRecommendations": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.next": true
  },
  "vim.leader": "<space>",
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["J"],
      "after": ["5", "j"]
    },
    {
      "before": ["K"],
      "after": ["5", "k"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["J"],
      "after": ["5", "j"]
    },
    {
      "before": ["K"],
      "after": ["5", "k"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["p"],
      "after": ["p", "g", "v", "y"]
    },
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    }
  ],
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "gitlens.currentLine.enabled": false,
  "explorer.openEditors.visible": 0,
  "explorer.autoReveal": false,
  "vim.disableExtension": false,
  "git.enableSmartCommit": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "go.useLanguageServer": true,
  "go.formatTool": "goimports",
  "vsicons.dontShowNewVersionMessage": true,
  "terraform.languageServer": {
    "enabled": true
  },
  "pgFormatter.keywordCase": "uppercase",
  "pgFormatter.commaBreak": true,
  "settingsSync.ignoredExtensions": [],
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "projectManager.git.baseFolders": ["~/Code"],
  "tabnine.experimentalAutoImports": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.fontFamily": "'SauceCodePro Nerd Font Mono', monospace",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.editor.tabActionCloseVisibility": false,
  "window.confirmSaveUntitledWorkspace": false,
  "python.analysis.typeCheckingMode": "standard",
  "workbench.colorTheme": "Catppuccin Mocha",
  "vs-kubernetes": {
    "vs-kubernetes.minikube-show-information-expiration": "2025-09-23T15:21:55.096Z"
  },
  "diffEditor.codeLens": true
}
