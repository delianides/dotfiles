#!/bin/bash

function cd () {
	if   [[ "x$*" == "x..." ]]; then
		cd ../..
			elif [[ "x$*" == "x...." ]]; then
			cd ../../..
			elif [[ "x$*" == "x....." ]]; then
			cd ../../../..
			elif [[ "x$*" == "x......" ]]; then
			cd ../../../../..
			elif [ -d ~/.autoenv ]; then
			source ~/.autoenv/activate.sh
			autoenv_cd "$@"
	else
		builtin cd "$@"
			fi
}
